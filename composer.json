{
  "name": "rugalib/ruga-std",
  "description": "Standard components for the ruga framework",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/rugalib",
  "type": "library",
  "require": {
    "php": "^7.4",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "laminas/laminas-config-aggregator": "^1",
    "laminas/laminas-servicemanager": "^3"
  },
  "authors": [
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\Std\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ruga\\Std\\Test\\": "test/src"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Std"
    }
  }
}